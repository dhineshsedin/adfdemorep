{
	"name": "pip_pipelineRAM",
	"properties": {
		"activities": [
			{
				"name": "Copy data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"additionalColumns": [
							{
								"name": "CourseUpdate",
								"value": "Physics"
							}
						],
						"sqlReaderQuery": "Select * from studentinfo where studentid = '2001'",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"StudentID"
							]
						},
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "StudentID",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "StudentID",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "CourseUpdate",
									"type": "String"
								},
								"sink": {
									"name": "Course",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Marks",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "Marks",
									"type": "Int32",
									"physicalType": "int"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DS_StudentInfo",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_destination",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}