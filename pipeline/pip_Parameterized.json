{
	"name": "pip_Parameterized",
	"properties": {
		"activities": [
			{
				"name": "Copy data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "@{pipeline().parameters.pip_schemaName}.@{pipeline().parameters.pip_TableName}"
					},
					{
						"name": "Destination",
						"value": "@{pipeline().parameters.pip_schemaName}.@{pipeline().parameters.pip_DestinationTableName}"
					},
					{
						"name": "UserPerformed",
						"value": "Ramamoorthy"
					}
				],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DS_Parameterized",
						"type": "DatasetReference",
						"parameters": {
							"TableName": {
								"value": "@pipeline().parameters.pip_TableName",
								"type": "Expression"
							},
							"SchemaName": {
								"value": "@pipeline().parameters.pip_schemaName",
								"type": "Expression"
							},
							"Servername": {
								"value": "@pipeline().parameters.pip_Servername",
								"type": "Expression"
							},
							"DBname": {
								"value": "@pipeline().parameters.pip_DBName",
								"type": "Expression"
							},
							"username": {
								"value": "@pipeline().parameters.pip_username",
								"type": "Expression"
							},
							"Password": {
								"value": "@pipeline().parameters.pip_password",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_Parameterized",
						"type": "DatasetReference",
						"parameters": {
							"TableName": {
								"value": "@pipeline().parameters.pip_DestinationTableName",
								"type": "Expression"
							},
							"SchemaName": {
								"value": "@pipeline().parameters.pip_schemaName",
								"type": "Expression"
							},
							"Servername": {
								"value": "@pipeline().parameters.pip_Servername",
								"type": "Expression"
							},
							"DBname": {
								"value": "@pipeline().parameters.pip_DBName",
								"type": "Expression"
							},
							"username": {
								"value": "@pipeline().parameters.pip_username",
								"type": "Expression"
							},
							"Password": {
								"value": "@pipeline().parameters.pip_password",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"pip_Servername": {
				"type": "string"
			},
			"pip_DBName": {
				"type": "string"
			},
			"pip_username": {
				"type": "string"
			},
			"pip_password": {
				"type": "string"
			},
			"pip_schemaName": {
				"type": "string"
			},
			"pip_TableName": {
				"type": "string"
			},
			"pip_DestinationTableName": {
				"type": "string"
			}
		},
		"annotations": []
	}
}